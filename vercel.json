{
  "buildCommand": "pnpm --filter @solo-leveling/shared build && pnpm --filter @solo-leveling/web build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
